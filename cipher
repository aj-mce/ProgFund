		import java.util.Scanner;
		import java.util.Random;


public class Cipher
{
	public static void main(String[] args)
	{
		  
		int temp,first,last,number = 0;
		Scanner scan = new Scanner( System.in );
		System.out.println("Enter 5 digits between 0 to 19");
		  
		  
		for(int i =0; i < 5 ; i++)
		{
			temp = scan.nextInt();
			if(temp >= 0 && temp <= 19 )
			{
			number = number + temp;
			}
			else
			{
				System.out.println("Please read directions and try again!");
				System.exit(0);
			}
		}
		//key 
		Random random = new Random();
		int rand = random.nextInt(10);
		System.out.println("The Key Is :"+rand);
		  
		first = number/10;
		last = number%10;
		  
		//encryption
		first = first + rand;
		last = last + rand;
		first = first%10;
		last = last%10;
		  
		//output
		System.out.println("The Encoded number is : "+first+last);
		  
	}

}
